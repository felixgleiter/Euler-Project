rows = [[75],[95,64],[17,47,82],[18,35,87,10],[20,4,82,47,65]];

for (var n = rows.length - 1; n > 0; n--) {
  for (var i = 0; i < rows[n - 1].length; i++) {
    rows[n - 1][i] += Math.max(rows[n][i], rows[n][i + 1]);
  }
}

console.log(rows[0][0]);

